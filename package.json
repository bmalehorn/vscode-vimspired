{
  "name": "vimspired",
  "displayName": "Vimspired",
  "description": "Create your own Vim-inspired keybindings!",
  "version": "6.0.0",
  "publisher": "bmalehorn",
  "engines": {
    "vscode": "^1.28.0"
  },
  "repository": {
    "url": "https://github.com/bmalehorn/vscode-bext"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Vimspired",
      "properties": {
        "vimspired.keybindings": {
          "type": "object",
          "description": "Keybindings map, key â†’ vscode command",
          "default": {
            "i": "vimspired.enterInsert"
          }
        }
      }
    },
    "commands": [
      {
        "command": "vimspired.enterNormal",
        "title": "Vimspired: normal mode"
      },
      {
        "command": "vimspired.enterInsert",
        "title": "Vimspired: insert mode"
      }
    ],
    "keybindings": [
      {
        "key": "Escape",
        "command": "vimspired.enterNormal",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "compile": "webpack --mode none",
    "watch": "webpack --mode none --watch",
    "test-compile": "tsc -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "extensionDependencies": [
    "stkb.rewrap",
    "brunnerh.insert-unicode",
    "eamodio.gitlens"
  ],
  "devDependencies": {
    "@types/lodash": "^4.14.134",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.7",
    "husky": "^2.4.0",
    "lodash": "^4.17.11",
    "prettier": "^1.18.0",
    "pretty-quick": "^1.11.0",
    "ts-loader": "^6.0.2",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.1",
    "vscode": "^1.1.34",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2"
  },
  "__metadata": {
    "id": "20b6ea6f-43c5-4004-b393-c237975cbd76",
    "publisherDisplayName": "Brian Malehorn",
    "publisherId": "1215635c-400c-4cc8-96d3-f2d5da4774a4"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
